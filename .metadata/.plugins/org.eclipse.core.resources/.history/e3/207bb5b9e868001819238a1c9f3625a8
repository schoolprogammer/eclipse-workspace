package time;

public class timeer {
	public static int timer = 300;
	
	
	public static void main(String[]args) {
			int temp;
			
			for(int i = -200; i <= timer; i++) {
			timer--;

			System.out.println(timer);	

			try {
				Thread.sleep(10);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
			
		
			
			}
		
	}
}
