package time;

public class timeer {
	public static long timer = 300;
	public static void main(String[]args) {
		
		
		while(timer>=0) {
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
			for(int i = 0; i <= timer; i++) {
			System.out.println(timer-1);
			}
		}
	}
}
