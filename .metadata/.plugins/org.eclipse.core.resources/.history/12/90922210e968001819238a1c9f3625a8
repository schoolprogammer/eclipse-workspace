package time;

public class timeer {
	public static int timer = 300;
	
	
	public static void main(String[]args) {
			int temp;
			
			while(timer>0) {
			timer--;

			System.out.println(timer);
			try {
				Thread.sleep(10);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}	
			}
		
	}
}
